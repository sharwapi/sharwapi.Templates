{
  "$schema": "http://json.schemastore.org/template",
  "author": "SharwAPI",
  "classifications": [ "Common", "Library" ],
  "name": "SharwAPI Plugin Template",
  "description": "A template for creating SharwAPI plugins.",
  "identity": "SharwAPI.Plugin.Template",
  "shortName": "sharwapiplugin",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  
  "sourceName": "Plugin_Name",
  "preferNameDirectory": true,

  "symbols": {
    "Author": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "description": "The author of the plugin",
      "replaces": "Plugin_Author",
      "fileRename": "Plugin_Author"
    }
  }
}